{
  "name": "@amzn/dcv-access-console-web-client-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf build && rm -rf node_modules && rm -rf package-lock.json && rm -rf coverage && rm -rf src/generated-src && rm -rf model && rm -rf .next",
    "prebuild": "mkdir -p model && cp ../../dcv-access-console-model/model/* model",
    "build": "npm run generate && npm run generate-msw && npm run generate-search-tokens && next build",
    "postbuild": "npm run test",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006 --config-dir ../server/.storybook",
    "build-storybook": "storybook build",
    "test": "jest --coverage --transformIgnorePatterns \"node_modules/(?!your-module-name)/\"",
    "coverage": "jest --coverage --transformIgnorePatterns \"node_modules/(?!your-module-name)/\"",
    "generate": "openapi-generator-cli generate -i model/dcv-access-console-handler-api.yaml -g typescript-axios -o src/generated-src/client --global-property models,apis,supportingFiles=api.ts:base.ts:common.ts:configuration.ts:index.ts",
    "generate-mock": "msw-auto-mock model/dcv-access-console-handler-api.yaml -o src/generated-src/msw/mock.js --base-url http://localhost:8080 --node",
    "generate-msw": "msw init src/generated-src/msw/ --save && npm run generate-mock",
    "gen-desc-sesns-tokens": "ts-node src/scripts/generate_filter_tokens.tsx model/dcv-access-console-handler-api.yaml src/generated-src/client/session_search_tokens.ts DescribeSessionsUIRequestData",
    "gen-desc-servs-tokens": "ts-node src/scripts/generate_filter_tokens.tsx model/dcv-access-console-handler-api.yaml src/generated-src/client/server_search_tokens.ts DescribeServersUIRequestData",
    "gen-desc-sessn-temp-tokens": "ts-node src/scripts/generate_filter_tokens.tsx model/dcv-access-console-handler-api.yaml src/generated-src/client/session_template_search_tokens.ts DescribeSessionTemplatesRequestData",
    "gen-desc-usr-grps-tokens": "ts-node src/scripts/generate_filter_tokens.tsx model/dcv-access-console-handler-api.yaml src/generated-src/client/user_groups_search_tokens.ts DescribeUserGroupsRequestData",
    "gen-desc-usrs-tokens": "ts-node src/scripts/generate_filter_tokens.tsx model/dcv-access-console-handler-api.yaml src/generated-src/client/users_search_tokens.ts DescribeUsersRequestData",
    "generate-search-tokens": "npm run gen-desc-sesns-tokens & npm run gen-desc-servs-tokens & npm run gen-desc-sessn-temp-tokens & npm run gen-desc-usr-grps-tokens & npm run gen-desc-usrs-tokens"
  },
  "dependencies": {
    "@cloudscape-design/components": "^3.0.270",
    "@cloudscape-design/global-styles": "^1.0.9",
    "@types/node": "18.16.3",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "autoprefixer": "10.4.14",
    "encoding": "^0.1.13",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.0",
    "moment": "^2.29.4",
    "msw-auto-mock": "^0.14.0",
    "next": "13.5.5",
    "next-auth": "^4.22.5",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.5",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "4.7.1",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@cloudscape-design/jest-preset": "^2.0.0",
    "@faker-js/faker": "^8.0.2",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "~7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/testing-library__jest-dom": "5.14.5",
    "@testing-library/dom": "<10.0.0",
    "axios": "^0.28.1",
    "ajv": "^8.12.0",
    "babel-jest": "^29.5.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "msw": "^1.2.1",
    "storybook": "^7.4.1",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=16.14.0 <17.0.0"
  },
  "files": [
    ".next/",
    "access-console-web-client.properties",
    "access-console-web-client-secrets.properties",
    "access-console-web-client-advanced.properties",
    "public/",
    "run-server"
  ],
  "msw": {
    "workerDirectory": "src/generated-src/msw"
  },
  "bundledDependencies": true
}
